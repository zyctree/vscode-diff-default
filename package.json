{
	"name": "diffdefault",
	"publisher": "zyctree",
	"repository": {
		"type": "git",
		"url": "https://github.com/zyctree/vscode-diff-default"
	},
	"displayName": "Diff Default",
	"description": "diff the open file with the default file or folder",
	"version": "0.0.8",
	"engines": {
		"vscode": "^1.7.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:diffdefault"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "diffdefault",
				"title": "Diff Default"
			}
		],
		"keybindings": [
			{
				"command": "diffdefault",
				"key": "f7",
				"when": "editorTextFocus"
			}
		],
		"configuration": [
			{
				"title": "DiffDefault",
				"properties": {
					"diffdefault.rules": {
						"type": "array",
						"description": "array of rules, each one need to be an array of two",
						"items": {
							"type": "array",
							"rule": [
								{
									"type": "string"
								},
								{
									"type": "string"
								}
							]
						}
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.7.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
